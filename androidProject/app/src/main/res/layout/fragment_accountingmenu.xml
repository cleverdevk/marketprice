<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back">

    <Button
        android:id="@+id/btnAccountingView"
        android:layout_width="217dp"
        android:layout_height="217dp"
        android:layout_marginTop="111dp"
        android:layout_marginBottom="78dp"
        android:background="@drawable/circle_border"
        android:drawableTop="@drawable/accounting_search2"
        android:text="둘러보기"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/btnAccountingWrite"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnAccountingWrite"
        android:layout_width="217dp"
        android:layout_height="217dp"
        android:layout_marginBottom="106dp"
        android:background="@drawable/circle_border"
        android:drawableTop="@drawable/accounting_write"
        android:text="작성하기"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAccountingView" />
</android.support.constraint.ConstraintLayout>